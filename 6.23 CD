#include<iostream>
using namespace std;
#define INF 0x3f3f3f3f
int d[1001][1001],p[1001][1001];
int main()
{
    int m,n;
    char u,v,s,t;
    int u1,v1;
    int s1,t1;
    int w;
    cin>>m>>n;
    for(int i=1;i<m;i++)
    {
        for(int j=1;j<=m;j++)
        {
            dist[i][j]=INF;
            pre[i][j]=j;
        }
    }
    for(int i=1;i<=n;i++)
    {
        cin>>u>>v>>w;
        u1=u-64;
        v1=v-64;
        dist[u1][v1]=w;
    }
    cin>>s>>t;
    s1=s-64;
    t1=t-64;
    for(k=1;k<=m;k++){
        for(i=1;i<=m;i++){
            for(j=1;j<=m;j++){
                if(dist[i][j]>dist[i][k]+dist[k][j]){
                    dist[i][j]=dist[i][k]+dist[k][j];
                    pre[i][j]=pre[i][k];
                }
            }
        }
    }
    cout<<dist[s1][t1];
    cout<<s<<" ";
    int vec=pre[s1][t1];
    while(vec!=t1){
        cout<<char(v+64)<<" ";
        vec=p[v][t1];
    }
    cout<<t;
}
