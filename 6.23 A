#include<iostream>
using namespace std;
#define MAX 1001
int BSTcount(int *a,int key,int n)
{
    int count=0;
    int l=0,middle,r=n-1;
    while(l<=r)
    {
        count++;
        middle=(l+r)/2;
        if(key>a[middle])
            l=middle+1;
        else if(key<a[middle])
            r=middle-1;
        else
            return count;
    }
    return -100;
}
int main()
{
    int num;
    cin>>num;
    int a[MAX]={0};
    for(int i=0;i<num;i++)
    {
        cin>>a[i];
    }
    int Key=0;
    cin>>Key;
    if(BSTcount(a,Key,num)==-100)
        cout<<"NO";
    else
        cout<<BSTcount(a,Key,num);
    return 0;
}
