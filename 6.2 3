#include<iostream>
using namespace std;
void DtoB(int n)
{
    int i,j=0;
    int a[1000];
    i=n;
    while(i)
    {
     a[j]=i%2;
     i/=2;
     j++;
     
    }
    for(i=j-1;i>=0;i--)
     cout<<a[i];
     cout<<endl;
}
int main()
{
    int n;
    cin>>n;
    int num[n];
    for(int i=0;i<n;i++)
    {
        cin>>num[i];
    }
    for(int i=0;i<n;i++)
    {
        DtoB(num[i]);
    }
    return 0;
}
