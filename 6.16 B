#include <iostream>
#include <cstring>
using namespace std;
void judge(int a[],int n){
    if(n%2==0){
        if(a[n/2]>a[n])
               cout<<"False"<<endl;
        for(int i=n/2-1;i>=0;i--)
            if(a[i]>a[2*i]||a[i]>a[2*i+1])
                cout<<"False"<<endl;
    }
    else{
        for(int i=n/2;i>=0;i--){
            if(a[i]>a[2*i]||a[i]>a[2*i+1])
                cout<<"False"<<endl;
        }
    }
    cout<<"True"<<endl;
}
int main()
{
    int n,j=0,m=0;
    int len=0;
    cin>>n;
    char ch[200];
    int xulie[101]={0};
    for(int i=0;i<n;i++)
    {
        cin>>ch;
        len=0;
        xulie[101]={0};
        while(ch[j]!='\n')
        {
            if(ch[j]==' ')
            {
                len++;
                m=0;
            }
            else
            {
                xulie[len]=xulie[len]*10^m+(ch[j]-48);
                m++;
            }
        }
        judge(xulie,len);
    }
    return 0;
}
